                    textOnly="&lt;p style=&quot;text-align: center;&quot;>Andrew McArthur, Ph.D., Professor in Biochemistry and Biomedical Sciences at McMaster University&lt;/p>&#xd;&#xa;"/>
                <pullquote_349b
                    jcr:created="{Date}2018-06-18T09:23:47.197-07:00"
                    jcr:createdBy="velazquez@hpe.com"
                    jcr:lastModified="{Date}2022-07-22T18:22:13.849Z"
