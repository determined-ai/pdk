                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="hpeweb/components/content/textOnlyComponent"
                    designAccentColor="gray"
                    textIsRich="true"
                    textOnly="&lt;p>&lt;span class=&quot;body-copy-large&quot;>With the new solution in place, client satisfaction at Datarewind is reportedly at an all-time high. “The best part is when clients contact us&amp;nbsp;about how happy they are with the new solution,” concludes Rickard&amp;nbsp;Johansson.&lt;/span>&lt;/p>&#xd;&#xa;"/>
                <textonlycomponent_fe
                    jcr:created="{Date}2018-06-18T09:28:54.456-07:00"
                    jcr:createdBy="velazquez@hpe.com"
                    jcr:lastModified="{Date}2022-06-13T15:16:37.104Z"
