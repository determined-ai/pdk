                    textIsRich="true"
                    textOnly="&lt;h3>IN THIS ARTICLE&lt;/h3>&#xd;&#xa;&lt;ul>&#xd;&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>Eclit has selected the HPE GreenLake platform to launch a new suite of cloud services and transform part of its existing IT estate&lt;/span>&lt;/li>&#xd;&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>Eclit is now making available to its customers an extensive cloud services portfolio that includes a broad spectrum of flexible, on-demand IT infrastructure&lt;/span>&lt;/li>&#xd;&#xa;&lt;/ul>&#xd;&#xa;"
                    topPadding="30"/>
                <horizontalgrayruleli
                    jcr:created="{Date}2018-06-18T09:16:54.243-07:00"
                    jcr:createdBy="velazquez@hpe.com"
                    jcr:lastModified="{Date}2018-06-18T09:18:45.987-07:00"
