                    textOnly="&lt;h2>The HPE Securities Analyst Meeting 2022 is scheduled for October 19&lt;/h2>&#xd;&#xa;"/>
                <textonlycomponent_e9
                    jcr:created="{Date}2018-06-22T11:24:07.263-07:00"
                    jcr:createdBy="velazquez@hpe.com"
                    jcr:lastModified="{Date}2022-10-04T14:21:25.411Z"
                    jcr:lastModifiedBy="christine.seales@hpe.com"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="hpeweb/components/content/textOnlyComponent"
                    designAccentColor="gray"
                    textIsRich="true"
                    textOnly="&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;b>HOUSTON, Texas&lt;/b> – &lt;b>October 4, 2022&lt;/b> – &lt;a href=&quot;http://www.hpe.com&quot; target=&quot;_blank&quot;>Hewlett Packard Enterprise&lt;/a> (NYSE: HPE) will host its Securities Analyst Meeting on October 19, 2022. Join the live webcast to hear Antonio Neri, president and CEO, and Tarek Robbiati, executive vice president and CFO, discuss HPE’s vision, strategy, and financial outlook.&amp;nbsp;&lt;/span>&lt;b>&lt;/b>&lt;/p>&#xd;&#xa;"
                    topPadding="30"/>
                <pullquote_349b
                    jcr:created="{Date}2018-06-18T09:23:47.197-07:00"
                    jcr:createdBy="velazquez@hpe.com"
                    jcr:lastModified="{Date}2022-10-04T14:22:10.740Z"
                    jcr:lastModifiedBy="christine.seales@hpe.com"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="hpeweb/components/content/homepage/design2/pullQuote"
                    attribution=" "
                    bottomPadding="30"
                    color="style-bg-slate"
                    enableSocialSharing="true"
                    quoteLayout="standard"
                    quoteText="&lt;p>Hewlett Packard Enterprise will host the HPE Securities Analyst Meeting 2022 on October 19, 2022&lt;/p>&#xa;"
                    topPadding="30">
                    <image
                        jcr:lastModified="{Date}2022-10-04T14:22:10.740Z"
                        jcr:lastModifiedBy="christine.seales@hpe.com"
                        jcr:primaryType="nt:unstructured"
