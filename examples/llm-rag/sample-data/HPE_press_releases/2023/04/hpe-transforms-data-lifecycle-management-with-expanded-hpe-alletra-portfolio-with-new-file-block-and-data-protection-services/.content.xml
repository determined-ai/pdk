                    sling:resourceType="hpeweb/components/content/textOnlyComponent"
                    designAccentColor="gray"
                    textIsRich="true"
                    textOnly="&lt;h2>New data services built on HPE Alletra Storage MP offer intuitive cloud experience, high performance scale-out storage, and hybrid data protection delivered through HPE GreenLake&amp;nbsp;&lt;b>&lt;/b>&lt;/h2>&#xd;&#xa;"/>
                <textonlycomponent_e9
                    jcr:created="{Date}2018-06-22T11:24:07.263-07:00"
                    jcr:createdBy="velazquez@hpe.com"
                    jcr:lastModified="{Date}2023-04-03T16:22:40.597Z"
                    jcr:lastModifiedBy="christine.seales@hpe.com"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="hpeweb/components/content/textOnlyComponent"
                    designAccentColor="gray"
                    textIsRich="true"
                    textOnly="&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;b>HOUSTON, Texas – April 4, 2023 &lt;/b>– &lt;a href=&quot;https://www.hpe.com/us/en/home.html&quot; target=&quot;_blank&quot;>Hewlett Packard Enterprise&lt;/a> (NYSE: HPE) today announced new file, block, disaster and backup recovery data services designed to help customers eliminate data silos, reduce cost and complexity, and improve performance. The new file storage data services deliver scale-out, enterprise-grade performance for data-intensive workloads, and the expanded block services provide mission-critical storage with mid-range economics. &lt;br>&#xd;&#xa;&lt;/span>&lt;/p>&#xd;&#xa;"
                    topPadding="30"/>
                <imageonly
                    jcr:created="{Date}2023-04-03T16:23:13.450Z"
                    jcr:createdBy="christine.seales@hpe.com"
                    jcr:lastModified="{Date}2023-04-03T17:05:40.270Z"
                    jcr:lastModifiedBy="christine.seales@hpe.com"
