                    gradientAspectRatio="img-container-9-6"
                    gradientPlacement="top-right"
                    imageAlignment="left"
                    imageAspectRatio="native"
                    isZoom="false"
                    mode="mode-image"
                    zoomBackground="white">
                    <ctaButtons jcr:primaryType="nt:unstructured">
                        <item0
