{
  "pipeline": {
    "name": "finbert-prep"
  },
  "description": "Prepares the data for training ",
  "input": {
    "pfs": {
      "name": "data",
      "repo": "finbert-data",
      "branch": "master",
      "glob": "/"
    }
  },
  "transform": {
    "cmd": [
      "/bin/sh"
    ],
    "stdin": [
      "python datasets.py --input_dir /pfs/data --output_dir /pfs/out"
    ],
    "image": "pachyderm/pdk:sentiment-analysis-prep-v0.0.1"
  },
  "autoscaling": true
}
