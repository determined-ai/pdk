{
  "pipeline": {
    "name": "finbert-prep"
  },
  "description": "Prepares the data for training ",
  "input": {
    "pfs": {
      "name": "data",
      "repo": "finbert-data",
      "branch": "master",
      "glob": "/"
    }
  },
  "transform": {
    "cmd": [
      "/bin/sh"
    ],
    "stdin": [
      "python datasets.py --input_dir /pfs/data --output_dir /pfs/out"
    ],
    "image": "vmtyler/pdk:finbert_prep_0.0.4"
  }}
